# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'LispOSX' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for LispOSX
  pod 'LispCore', :path => '../'

  # Pods for LispOSXTests
  target 'LispOSXTests' do
    pod 'LispCore', :path => '../', :testspecs => ['Tests']
  end

end
